{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Log",
	"scopeName": "code.log",
	"patterns": [
		{ "include": "#gcc" },
		{ "include": "#clang" },
		{ "include": "#msvc" },
		{ "include": "#cmake" }
	],
	"repository": {
		"gcc": {
			"patterns": [
				{
					"match": "[Ee]rror: .*$",
					"name": "markup.other.log.error"
				},
				
				
				{
					"match": "undefined reference to `.*'",
					"name": "markup.other.log.error"
				},
				{
					"match": "[Mm]ultiple definition of `.*'",
					"name": "markup.other.log.error"
				},
				{
					"match": "[Ww]arning: .*$",
					"name": "markup.other.log.warn"
				},
				{
					"match": "[Ww]arn: .*$",
					"name": "markup.other.log.warn"
				},
				{
					"match": "[Nn]ote: .*$",
					"name": "markup.other.log.info"
				},
				{
					"match": "[Ii]nfo: .*$",
					"name": "markup.other.log.info"
				},
				{
					"begin": "In function ",
					"end": ":",
					"captures": { "0": { "name": "markup.other.log.info" } },
					"contentName": "meta.embedded.block.cpp",
					"patterns": [ {"include": "source.cpp"} ]
				}
			]
		},
		"clang": {
			"patterns": [
			]
		},
		"msvc": {
			"patterns": [
				{
					"match": "[Ee]rror C[0-9]+: .*$",
					"name": "markup.other.log.error"
				},
				{
					"match": "[Ee]rror LNK[0-9]+: .*$",
					"name": "markup.other.log.error"
				},
				{
					"match": "[Ww]arning C[0-9]+: .*$",
					"name": "markup.other.log.warn"
				},
				{
					"match": "[Nn]ote: .*$",
					"name": "markup.other.log.info"
				}
			]
		},
		"cmake": {
			"patterns": [
				{
					"match": "^\\[[A-Za-z]+\\]",
					"name": "markup.other.log.debug"
				},
				{
					"match": "\\[[0-9]+/[0-9]+\\]",
					"name": "markup.other.log.highlight"
				},
				{
					"match": "FAILED:",
					"name": "markup.other.log.error"
				},
				{
					"match": "Error.*",
					"name": "markup.other.log.error"
				}
			]
		}
	}
}



